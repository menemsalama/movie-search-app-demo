<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Movie Search App</title>
</head>
<body>

  <h1>Results page</h1>
  <h2>From <a href="http://www.omdbapi.com/" target="_blank">omdb</a> APIs</h2>
  <% data["Search"].forEach(function(movie) { %>
    <li>
      <strong><%= movie["Title"] %></strong> <%= movie["Year"] %>
    </li>
    <% }) %>

    <h3>
      <a href="/">Back</a>
    </h3>
  </body>
  </html>
